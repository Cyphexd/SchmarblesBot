{
  "name": "schmarblesbot",
  "version": "0.1.0",
  "description": "A simple Marbles Bot!",
  "author": "iKasu",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
